{
    "manifest_version": 2,
    "name": "niconico Darkmode",
    "version": "1.1.3",
    "description": "ニコニコサイトをダークモード化する拡張機能",
    "icons": {
        "16": "images/icon16.png",
        "32": "images/icon32.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png",
        "500": "images/icon500.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://www.nicovideo.jp/*",
                "https://live2.nicovideo.jp/create*"
            ],
            "js":["js/jquery-3.5.1.min.js","js/main.js"],
            "style_for/css":["parts/v120_header.css"],
            "run_at": "document_start"
        },
        {
            "matches": [
                "https://www.nicovideo.jp/",
                "https://www.nicovideo.jp/?*"
            ],
            "css": [
                "style_for/top/social-top.css"
            ]
        },
        {
            "matches": [
                "https://www.nicovideo.jp/recommendnews",
                "https://www.nicovideo.jp/recommendnews*"
            ],
            "css": [
                "style_for/top/recommendnews.css"
            ]
        },
        {
            "matches": [
                "https://www.nicovideo.jp/video_top*",
                "https://www.nicovideo.jp/video_top?*"
            ],
            "css": [
                "style_for/video/video_top.css"
            ]
        },
        {
            "matches": [
                "https://www.nicovideo.jp/video_top/genre*"
            ],
            "css": [
                "style_for/video/genre.css"
            ]
        },
        {
            "matches": [
                "https://www.nicovideo.jp/watch/*",
                "https://nicoad.nicovideo.jp/video/publish/*"
            ],
            "css": [
                "style_for/video/watch.css"
            ]
        },
        {
            "matches": [
                "https://www.nicovideo.jp/search/*",
                "https://www.nicovideo.jp/tag/*",
                "https://www.nicovideo.jp/mylist_search/*"
            ],
            "css": [
                "style_for/video/serch.css"
            ]
        },
        {
            "matches": [
                "https://www.nicovideo.jp/newarrival*",
                "https://www.nicovideo.jp/recent*",
                "https://www.nicovideo.jp/related_tag*"
            ],
            "css": [
                "style_for/video/video-page.css"
            ]
        },
        {
            "matches": [
                "https://www.nicovideo.jp/rss*"
            ],
            "css": [
                "style_for/top/rss.css"
            ]
        },
        {
            "matches": [
                "https://www.nicovideo.jp/my*",
                "https://www.nicovideo.jp/user/*"
            ],
            "css": [
                "style_for/video/mypage/mypage.css"
            ]
        },
        {
            "matches": [
                "https://www.nicovideo.jp/series/*"
            ],
            "css": [
                "style_for/video/mypage/series.css"
            ]
        },
        {
            "matches": [
                "https://www.nicovideo.jp/ranking*"
            ],
            "css": [
                "style_for/video/ranking.css"
            ]
        },
        {
            "matches": [
                "https://www.nicovideo.jp/recommendations*"
            ],
            "css": [
                "style_for/video/recommendations.css"
            ]
        },
        {
            "matches": [
                "https://www.nicovideo.jp/openlist*"
            ],
            "css": [
                "style_for/video/openlist.css"
            ]
        },
        {
            "matches": [
                "https://www.nicovideo.jp/ranking_graph*"
            ],
            "css": [
                "style_for/video/ranking_graph.css"
            ]
        },
        {
            "matches": [
                "https://www.nicovideo.jp/video_catalog*"
            ],
            "css": [
                "style_for/video/other.css"
            ]
        },
        {
            "matches": [
                "https://live.nicovideo.jp/",
                "https://live.nicovideo.jp/?*",
                "https://live2.nicovideo.jp/",
                "https://live2.nicovideo.jp/?*"
            ],
            "css": [
                "style_for/parts/header.css",
                "style_for/live/parts/live_parts.css",
                "style_for/live/parts/card.css",
                "style_for/live/live_top.css"
            ]
        },
        {
            "matches": [
                "https://live2.nicovideo.jp/focus",
                "https://live2.nicovideo.jp/focus?*"
            ],
            "css": [
                "style_for/parts/header.css",
                "style_for/live/parts/live_parts.css",
                "style_for/live/parts/card.css",
                "style_for/live/forcus.css"
            ]
        },
        {
            "matches": [
                "https://live.nicovideo.jp/recent",
                "https://live.nicovideo.jp/recent?*"
            ],
            "css": [
                "style_for/parts/header.css",
                "style_for/live/parts/live_parts.css",
                "style_for/live/parts/card.css",
                "style_for/live/recent.css"
            ]
        },
        {
            "matches": [
                "https://site.live.nicovideo.jp/recent/namagame*"
            ],
            "css": [
                "style_for/live/site-live/namagame.css"
            ]
        },
        {
            "matches": [
                "https://live.nicovideo.jp/timetable*"
            ],
            "css": [
                "style_for/parts/header.css",
                "style_for/live/parts/live_parts.css",
                "style_for/live/timetable.css"
            ]
        },
        {
            "matches": [
                "https://live.nicovideo.jp/ranking*"
            ],
            "css": [
                "style_for/parts/header.css",
                "style_for/live/parts/live_parts.css",
                "style_for/live/ranking.css"
            ]
        },
        {
            "matches": [
                "https://live2.nicovideo.jp/watch/*"
            ],
            "css": [
                "style_for/parts/header.css",
                "style_for/live/parts/live_parts.css",
                "style_for/live/watch.css"
            ]
        },
        {
            "matches": [
                "https://live.nicovideo.jp/follow*"
            ],
            "css": [
                "style_for/parts/header.css",
                "style_for/live/parts/live_parts.css",
                "style_for/live/parts/card.css",
                "style_for/live/follow.css"
            ]
        },{
            "matches": [
                "https://live.nicovideo.jp/my*"
            ],
            "css": [
                "style_for/parts/header.css",
                "style_for/live/parts/live_parts.css",
                "style_for/live/my.css"
            ]
        },{
            "matches": [
                "https://live2.nicovideo.jp/create*"
            ],
            "css": [
                "style_for/live/create.css"
            ]
        },
        {
            "matches": [
                "https://live.nicovideo.jp/search*"
            ],
            "css": [
                "style_for/parts/header.css",
                "style_for/live/parts/live_parts.css",
                "style_for/live/search.css"
            ]
        },
        {
            "matches": [
                "https://live.nicovideo.jp/history*"
            ],
            "css": [
                "style_for/parts/header.css",
                "style_for/live/parts/live_parts.css",
                "style_for/live/history.css"
            ]
        },
        {
            "matches": [
                "https://account.nicovideo.jp/login*"
            ],
            "css": [
                "style_for/account/login.css"
            ]
        }
    ]
    ,
    "permissions": [
        "storage"
    ]
}